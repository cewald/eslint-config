  "devDependencies": {
    "@cewald/eslint-config": "./",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "semantic-release": "^24.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.6.4",
    "deepmerge": "^4.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-tailwindcss": "^3.17.4"
  },
  "peerDependencies": {
    "eslint": "^9.9.1"
  },
  "lint-staged": {
